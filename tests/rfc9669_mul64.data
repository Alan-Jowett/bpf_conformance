-- asm
# RFC 9669 Section 4.1: MUL (ALU64) imm/reg semantics.

# imm variant
mov %r1, 6
mul %r1, 7
jne %r1, 42, fail

# reg variant
mov %r2, 14
mov %r3, 3
mul %r2, %r3
jne %r2, 42, fail

mov %r0, 1
exit
fail:
mov %r0, 0
exit
-- result
0x1
