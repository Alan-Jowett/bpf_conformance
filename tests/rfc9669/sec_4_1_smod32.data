-- asm
# RFC 9669 Section 4.1: SMOD (signed) ALU32 semantics (truncated division).

# -13 s% 3 == -1
mov32 %r1, -13
mov32 %r2, 3
smod32 %r1, %r2
jne %r1, 0xffffffff, fail

# modulo by zero leaves dst unchanged for ALU, but upper 32 bits are zeroed (ALU32)
mov %r3, -13
mov32 %r4, 0
smod32 %r3, %r4
jne %r3, 0x00000000fffffff3, fail

mov %r0, 1
exit
fail:
mov %r0, 0
exit
-- result
0x1
