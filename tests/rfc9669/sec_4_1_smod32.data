-- asm
# RFC 9669 Section 4.1: SMOD (signed) ALU32 semantics (truncated division).

# -13 s% 3 == -1
mov32 %r1, -13
mov32 %r2, 3
smod32 %r1, %r2
jne32 %r1, 0xffffffff, fail

# modulo by zero sets dst to 0 for ALU32
mov %r3, -13
mov32 %r4, 0
smod32 %r3, %r4
jne32 %r3, 0, fail

mov %r0, 1
exit
fail:
mov %r0, 0
exit
-- result
0x1
