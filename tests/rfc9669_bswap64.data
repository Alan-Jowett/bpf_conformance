-- asm
# RFC 9669 Section 4.2: bswap64 is unconditional byte swap (ALU64 END with imm=64).

lddw %r0, 0x1122334455667788
bswap64 %r0
lddw %r1, 0x8877665544332211
jne %r0, %r1, fail

mov %r0, 1
exit
fail:
mov %r0, 0
exit
-- result
0x1
