-- asm
mov r9, 1
rsh r9, 32
mov32 r0, 0
mov r1, 0xffffffff
# set r1 to 0x1fffffffe
or r1, r9
mov r2, 0xfffffffe
mov32 r3, 0
jsle32 r1, r2, exit # Not taken
jsle32 r1, r3, +1 # Taken
exit
mov32 r0, 1
mov r1, r2
jsle32 r1, r2, +1 # Taken
mov32 r0, 2
exit
-- result
0x1
